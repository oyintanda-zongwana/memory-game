{
    "version": 2,
    "builds": [
        {
            "src": "app.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/app.py"
        }
    ],
    "functions": {
        "app.py": {
            "memory": 1024,
            "maxDuration": 10
        }
    },
    "buildCommand": "pip install -r requirements.txt"
}